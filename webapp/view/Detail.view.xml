<mvc:View 
	controllerName="com.rizing.demo.mvc.controller.Detail" 
	xmlns="sap.m" 
	xmlns:mvc="sap.ui.core.mvc" >
	
	<Page
		showNavButton="false" 
	>
		
		<content>
			<ObjectHeader 
				title="{CompanyName}" 
				number="BusinessPartnerID: {BusinessPartnerID}" >
				
				<ObjectAttribute text="{Address/Street}" /> 
				<ObjectAttribute text="{Address/City}" /> 
				<ObjectAttribute text="{Address/Country}" /> 
			</ObjectHeader>
		</content>
	</Page>
	
		
</mvc:View>